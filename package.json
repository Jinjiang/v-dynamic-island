{
  "name": "vue-dynamic-island",
  "version": "0.1.0",
  "description": "A Vue (3.x) implementation of Dynamic Island.",
  "keywords": [
    "vue",
    "dynamic-island",
    "component"
  ],
  "author": "Jinjiang <zhaojinjiang@me.com>",
  "license": "MIT",
  "homepage": "https://github.com/Jinjiang/vue-dynamic-island",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jinjiang/vue-dynamic-island.git"
  },
  "bugs": {
    "url": "https://github.com/Jinjiang/vue-dynamic-island/issues"
  },
  "type": "module",
  "main": "./dist/dynamic-island.umd.cjs",
  "module": "./dist/dynamic-island.js",
  "exports": {
    ".": {
      "import": "./dist/dynamic-island.js",
      "require": "./dist/dynamic-island.umd.cjs"
    },
    "./style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "docs": "vue-tsc --noEmit && vite build",
    "build": "vue-tsc --noEmit && vite build --config vite.config.lib.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}
